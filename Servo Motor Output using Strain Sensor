#include <ESP32Servo.h>

int strainPin = 34;
int servoPin = 26;
int threshold = 250;

Servo myServo;

void setup() {
  Serial.begin(115200);
  myServo.attach(servoPin, 500, 2400);
  
}

void loop() {
  int sensorValue = analogRead(strainPin);
  Serial.println(sensorValue);

  if (sensorValue>threshold){
    myServo.write(90);
  } else {
    myServo.write(0);
  }
delay(100);

}
